<mvc:View xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
	controllerName="demoAribademoAriba.controller.View1" displayBlock="false">
	<App>
		<pages>
			<Page title="{i18n>title}">
				<content>
					<Table id="mytable" items="{/modelData}">
						<items>
							<ColumnListItem id="__item3">
								<cells>
									<Text text="{approvableUniqueName}"/>
									<Text text="{changeSequenceId}"/>
									<Text text="{changeType}"/>
									<Text text="{realm}"/>
									<Text text="{restResourceName}"/>
								</cells>
							</ColumnListItem>
						</items>
						<columns>
							<Column id="__column3">
								<header>
									<Label text="approvableUniqueName" id="__label3"/>
								</header>
							</Column>
							<Column id="__column4">
								<header>
									<Label text="changeSequenceId" id="__label4"/>
								</header>
							</Column>
							<Column id="__column5">
								<header>
									<Label text="changeType" id="__label5"/>
								</header>
							</Column>
							<Column id="__column6">
								<header>
									<Label text="realm" id="__label6"/>
								</header>
							</Column>
							<Column id="__column7">
								<header>
									<Label text="restResourceName" id="__label7"/>
								</header>
							</Column>
						</columns>
					</Table>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>